    <section id="contact" class="contact-section mt-5">
        <div class="var-header">
            <h3 class="var-title">İletişime Geçin</h3>
            <p class="var-text">MelodyWay İle İletişime Geçin</p>
        </div>
        <div class="var-footer">
            <div class="container">
                <div class="row">
                    <div class="contact-box">
                        <div class="row">
                            <div class="col-12 col-xl-6 col-lg-6 col-md-12 col-sm-12 contact-form-col">
                                <div class="contact-header">
                                    <h3 class="contact-title">İşletmeler İçin MelodyWay </h3>
                                </div>
                                <div class="contact-form">
                                    <form action="#" method="post">
                                        <div class="row">
                                            <div class="col-6">
                                                <input name="name" class="contact-input" placeholder="İsim..."
                                                    type="text">
                                            </div>
                                            <div class="col-6">
                                                <input class="contact-input" name="surname" placeholder="Soyisim..." type="text">
                                            </div>
                                            <div class="col-6">
                                                <input name="city" class="contact-input" placeholder="İliniz..."
                                                    type="text">
                                            </div>
                                            <div class="col-6">
                                                <input name="district" class="contact-input" placeholder="İlçeniz..."
                                                    type="text">
                                            </div>
                                            <div class="col-6">
                                                <input name="caffeName" class="contact-input" placeholder="Kafe Adı..."
                                                    type="text">
                                            </div>
                                            <div class="col-6">
                                                <input name="tableAmount" class="contact-input" placeholder="Masa Sayısı..."
                                                    type="text">
                                            </div>
                                            <div class="col-6">
                                                <input name="phone" class="contact-input" placeholder="Telefon..."
                                                    type="phone">
                                            </div>
                                            <div class="col-6">
                                                <input name="subject" class="contact-input" placeholder="Konu..."
                                                    type="text">
                                            </div>
                                            <div class="col-12">
                                                <textarea name="message" class="contact-textarea"
                                                    placeholder="Mesaj..."></textarea>
                                            </div>
                                            <div class="col-12 contact-btn-col">
                                                <button value="addMessage" name="addMessage" class="contact-btn">Gönder</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div class="col-12 col-xl-6 col-lg-6 col-md-12 col-sm-12 contact-information-col">
                                <div class="information-header">
                                    <h3 class="info-title">İletişim Bilgileri</h3>
                                </div>
                                <div class="info-infos">
                                    <ul>
                                        <li class="infos-li"><i class="bi bi-envelope-fill"></i> E-Posta:
                                            <%= fetch.email%>
                                        </li>
                                        <li class="infos-li"><i class="bi bi-telephone-fill"></i> Telefon:
                                            <%= fetch.phone%>
                                        </li>
                                        <li class="infos-li"><i class="bi bi-geo-alt-fill"></i> Adres:
                                            <%= fetch.adress%>
                                        </li>
                                    </ul>
                                </div>
                                <div class="info-socials">
                                    <a href="<%= fetch.facebook %>" target="_blank"><i
                                            class="bi bi-facebook"></i></a>
                                    <a href="<%= fetch.instagram%>" target="_blank"><i
                                            class="bi bi-instagram"></i></a>
                                    <a href="<%= fetch.twitter%>" target="_blank"><i class="bi bi-twitter"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

</main>
